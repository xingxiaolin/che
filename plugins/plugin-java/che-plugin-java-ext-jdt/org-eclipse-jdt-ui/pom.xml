                <artifactId>license-maven-plugin</artifactId>
                <configuration>
                    <excludes>
                        <exclude>**/org/eclipse/jdt/**</exclude>
                        <exclude>**/org/eclipse/che/**</exclude>
                        <exclude>**/org/eclipse/ltk/**</exclude>
                        <exclude>**/org/eclipse/ui/**</exclude>
                        <exclude>**/org/eclipse/search/**</exclude>
                        <exclude>**/templates/**</exclude>
                    </excludes>
                </configuration>
            </plugin>
        </plugins>
